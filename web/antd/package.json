{
  "name": "webpeer-antd",
  "version": "0.0.1",
  "description": "Web peer base admin interface based on ant design ",
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 88
  },
  "module": "dist/index.js",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "size-limit": "^4.10.1",
    "tsdef": "^0.0.14",
    "tslib": "^2.1.0",
    "typescript": "^5.7.3",
    "vite": "^6.3.3",
    "vite-dts": "^1.0.4",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "antd": "^5.24.1",
    "debounce": "^2.2.0",
    "react": ">=17",
    "react-dom": "^18.3.1",
    "use-breakpoint": "^4.0.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  },
  "homepage": "https://gridnine.com/",
  "author": {
    "name": "Avramenko Vladimir"
  },
  "keywords": [
    "webpeer"
  ],
  "publishConfig": {
    "access": "public"
  }
}
