{
  "name": "@webpeer/antd-admin",
  "version": "0.0.1",
  "description": "Web peer base admin interface based on ant design ",
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "printWidth": 88
  },
  "module": "dist/index.js",
  "devDependencies": {
    "size-limit": "^4.10.1",
    "tslib": "^2.1.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-dts": "^1.0.4",
    "tsdef": "^0.0.14",
    "vite-plugin-dts": "^4.5.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4"
  },
  "dependencies": {
    "@webpeer/core": "file:../core",
    "antd": "^5.24.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  },
  "homepage": "https://gridnine.com/",
  "author": {
    "name": "Avramenko Vladimir"
  },
  "keywords": [
    "webpeer"
  ],
  "publishConfig": {
    "access": "public"
  }
}
