  "module": "dist/index.js",
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "size-limit": "^4.10.1",
    "tsdef": "^0.0.14",
    "tslib": "^2.1.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.3",
    "vite-dts": "^1.0.4",
